<?xml version="1.0"?>
<launch>
  <arg name="gradviz" default="false"/>
  <param name="publish_gradient_visualization" value="$(arg gradviz)"/>

  <node name="gplanner" pkg="grasp_planner" type="grasp_planner_node" output="screen">
    
    <!--param name="gripper_file" value="$(find grasp_planner)/cfg/cylinder_05mm.cons"/ -->
    <param name="gripper_file" value="$(find grasp_planner)/cfg/yumi_cyl_03mm.cons"/>
    <!-- param name="gripper_file" value="$(find grasp_planner)/cfg/yumi_cylinder_05.cons"/ -->
    <param name="map_frame_name" value="sdf_map_frame"/>
    <param name="use_tf" value="true"/>

    <!-- asus camera -->
    <param name="depth_info_topic_name" value="/camera_1/depth/camera_info"/>
    <param name="depth_topic_name" value="/camera_1/depth/image_raw"/> 
    <!-- kinect2 camera -->
    <param name="depth_topic_name2" value="/camera_2/depth/image_raw"/>
    <param name="depth_info_topic_name2" value="/camera_2/depth/camera_info"/>

    <!-- SDF related parameters -->
    <param name="GridSizeX" value= "250"/>
    <param name="GridSizeY" value= "170"/>
    <param name="GridSizeZ" value= "90"/>
    <param name="PositiveTruncationDistance" value= "0.03" />
    <param name="NegativeTruncationDistance" value= "-0.003" />
    <param name="RobustStatisticCoefficient" value= "0.01" />
    <param name="OutputVolume" value= "false" />
    <param name="InteractiveMode" value= "false" />
    <param name="MaximumRaycastSteps" value= "360" /> 
    <param name="OutputTriangles" value= "false" />

    <param name="tf_world2sdf_x" value="0.32"/>
    <param name="tf_world2sdf_y" value="0.0"/>
    <param name="tf_world2sdf_z" value="0.15"/>
    <param name="tf_world2sdf_yaw" value="-1.570796"/>
    <param name="tf_world2sdf_pitch" value="0.0"/>
    <param name="tf_world2sdf_roll" value="0.0"/>
    
  </node>
  <node pkg="tf" type="static_transform_publisher" name="roipub"
        args="0.32 0 0.15 -1.570796 0.0 0.0 world sdf_map_frame 100" />
</launch>
